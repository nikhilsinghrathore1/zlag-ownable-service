{
  "version": 2,
  "name": "agents-api",
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}
